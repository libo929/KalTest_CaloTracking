.SUFFIXES: .$(SrcSuf)

.$(SrcSuf).$(ObjSuf):
	$(CXX) $(CXXFLAGS) -c $<

# # -----------------------------------------------------
# # Linux with egcs
# # -----------------------------------------------------

SOEXT         = so
LIBF2C	      = -l$(G77EGCS_LIBNAME)
FC	      = g77
CXX           = g++ -O2 -g
CC            = $(CXX)
CXXFLAGS      = -Wall -fPIC 		$(EXTRACXXOPT) 		$(shell root-config --cflags)

SOFLAGS       = -shared -rdynamic
LDFLAGS       = $(ANALLIB) $(GLIBS) -rdynamic
LIBS          = $(ROOTLIBS) -lm -ldl
GLIBS         = $(ROOTLIBS) $(ROOTGLIBS) -L/usr/X11R6/lib 		-lXpm -lX11 $(LIBF2C) -lm -ldl

