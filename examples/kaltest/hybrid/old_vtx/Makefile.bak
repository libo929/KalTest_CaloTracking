GCCVER = $(shell $(CXX) -dumpversion 2>/dev/null | sed -e 's;\..*;;')
ifeq ($(GCCVER),3)
   CXXFLAGS += -D__USEISOCXX__
endif

INSTALLDIR    = ../../../..
PACKAGENAME   = EXVTX
SOREV         = 2005.01
SRCS          = EXVTXKalDetector.$(SrcSuf) 		EXVTXMeasLayer.$(SrcSuf) 		EXVTXHit.$(SrcSuf)

OBJS	      =	$(subst .$(SrcSuf),.$(ObjSuf),$(SRCS)) 		$(PACKAGENAME)Dict.$(ObjSuf)

HDRS	      = $(subst .$(SrcSuf),.h,$(SRCS))

DICTNAME      = $(PACKAGENAME)Dict

LIBNAME       = $(PACKAGENAME)

LIBINSTALLDIR = $(INSTALLDIR)/lib
INCINSTALLDIR = $(INSTALLDIR)/include
INCPATH	      = -I. -I.. -I$(INCINSTALLDIR)
CXXFLAGS     += $(INCPATH) -O -g
SHLIBLDFLAGS  = $(DYLIBFLAGS)

all::

all:: lib$(LIBNAME).so.$(SOREV)

lib$(LIBNAME).so.$(SOREV):  $(OBJS) $(EXTRALIBRARYDEPS)
	$(RM) $@~
	@SONAME=`echo $@ | sed 's/\(lib[^\.]*\.so\.[0-9]*\)\(\.[0-9]*\)\{1,2\}/\1/'`; (set -x; \
		cd .; $(CC) -o ./$@~ $(SHLIBLDFLAGS) -Wl,-soname,$$SONAME $(OBJS) $(REQUIREDLIBS) -lc) || exit 1; \
		(set -x; $(RM) $$SONAME; $(LN) $@ $$SONAME); \
		true
	$(RM) $@
	$(MV) $@~ $@
	@if $(SOSYMLINK); then (set -x; \
	  $(RM) lib$(LIBNAME).so; \
	  $(LN) $@ lib$(LIBNAME).so); fi
	$(_NULLCMD_)
	$(_NULLCMD_)

cleandir::
	@MAJREV=`echo $(SOREV) | sed 's/\([0-9]*\)\(\.[0-9]*\)\{1,2\}/\1/'`; \
	set -x; $(RM) lib$(LIBNAME).so.$$MAJREV
	$(RM) lib$(LIBNAME).so.$(SOREV) lib$(LIBNAME).so

install:: lib$(LIBNAME).so.$(SOREV)
	@if [ -d $(DESTDIR)$(LIBINSTALLDIR) ]; then \
		set +x; \
	else \
		if [ -h $(DESTDIR)$(LIBINSTALLDIR) ]; then \
			(set -x; rm -f $(DESTDIR)$(LIBINSTALLDIR)); \
		fi; \
		(set -x; $(MKDIRHIER) $(DESTDIR)$(LIBINSTALLDIR)); \
	fi
	$(INSTALL) $(INSTALLFLAGS) $(INSTBINFLAGS) lib$(LIBNAME).so.$(SOREV) $(DESTDIR)$(LIBINSTALLDIR)
	@T=`echo $(DESTDIR)$(LIBINSTALLDIR)/lib$(LIBNAME).so.$(SOREV) | sed 's/\(lib[^\.]*\.so\.[0-9]*\)\(\.[0-9]*\)\{1,2\}/\1/'`;	  set -x; $(RM) $$T && $(LN) lib$(LIBNAME).so.$(SOREV) $$T
	@if $(SOSYMLINK); then (set -x; 	  $(RM) $(DESTDIR)$(LIBINSTALLDIR)/lib$(LIBNAME).so; \
	  $(LN) lib$(LIBNAME).so.$(SOREV) $(DESTDIR)$(LIBINSTALLDIR)/lib$(LIBNAME).so); fi

install:: $(HDRS)
	@if [ -d $(DESTDIR)$(INCINSTALLDIR) ]; then \
		set +x; \
	else \
		if [ -h $(DESTDIR)$(INCINSTALLDIR) ]; then \
			(set -x; rm -f $(DESTDIR)$(INCINSTALLDIR)); \
		fi; \
		(set -x; $(MKDIRHIER) $(DESTDIR)$(INCINSTALLDIR)); \
	fi
	@for flag in ${MAKEFLAGS} ''; do \
	  case "$$flag" in *=*) ;; --*) ;; *[i]*) set +e;; esac; done; \
	for i in $(HDRS); do \
		(set -x; $(INSTALL) $(INSTALLFLAGS) -m 644 $$i $(DESTDIR)$(INCINSTALLDIR)); \
	done

cleandir::
	@rm -f $(OBJS) core *.$(DllSuf) $(DICTNAME).$(SrcSuf) $(DICTNAME).h

depend:: $(SRCS) $(HDRS)
	for i in $(SRCS); do 	rmkdepend -a -- $(DEPENDFILES) -- $$i; done

distclean:: cleandir
	@rm -f $(OBJS) core *.$(DllSuf) $(DICTNAME).$(SrcSuf) $(DICTNAME).h *~
	@rm -f *.root Makefile

$(DICTNAME).$(SrcSuf): $(HDRS) LinkDef.h
	@echo "Generating dictionary ..."
	rootcint -f $(DICTNAME).$(SrcSuf)  		-c $(INCPATH) $(HDRS) LinkDef.h

