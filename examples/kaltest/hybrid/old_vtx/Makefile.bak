ROOT_MINORVERSION = 28
ROOT_PATCHLEVEL= 0
ROOT_FULLVERSION= 52800
G77EGCS_VERSION =
G77EGCS_MINORVERSION =
G77EGCS_PATCHLEVEL =
UNAME_MACHINE = x86_64
UNAME_SYSNAME = Linux
G77EGCS_LIBNAME = m

DllSuf        = $(SOEXT)
DylibSuf      = $(SOEXT)
